{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  },
  "ignorePatterns": ["**/*.d.ts", "**/*.d.tsx", "**/*.min.js"],
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "project": "./tsconfig.json"
      },
      "plugins": [
        "@typescript-eslint"
      ],
      "extends": [
        "airbnb-typescript"
      ],
      "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
      }
    }
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/require-default-props": "off",
    "react/default-props-match-prop-types": "off",
    "react/static-property-placement": "off",
    "max-len": "off",
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "react/function-component-definition": "off"
  },
  "globals": {
    "document": false,
    "$": false
  }
}